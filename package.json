{
  "name": "secorto.com",
  "version": "3.1.0",
  "description": "Página web de presentación de Sergio C. Orozco Torres",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "start": "npm run develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "test": "jest",
    "lint:styles": "stylelint 'src/**/*.jsx'",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:md": "remark .",
    "test-with-coverage": "jest --coverage && cat ./coverage/lcov.info | codacy-coverage",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "peerDependencies": {
    "babel-jest": "^24.0.0",
    "gatsby": "^2.0.111",
    "gatsby-source-filesystem": "^2.0.22",
    "prismjs": "^1.16.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^24.0.0",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-preset-gatsby": "^0.1.3",
    "codacy-coverage": "^3.4.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.13.0",
    "gatsby": "^2.0.111",
    "gatsby-plugin-styled-components": "^3.0.1",
    "gatsby-remark-prismjs": "^3.2.9",
    "gatsby-source-filesystem": "^2.0.22",
    "gatsby-transformer-remark": "^2.3.12",
    "husky": ">=1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.0.0",
    "jest-styled-components": "^6.3.0",
    "lint-staged": ">=8",
    "prettier": "^1.17.1",
    "prismjs": "^1.16.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-test-renderer": "^16.6.1",
    "remark": "^10.0.1",
    "remark-cli": "^6.0.1",
    "remark-frontmatter": "^1.3.1",
    "remark-lint": "^6.0.4",
    "remark-message-control": "^4.1.1",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "remark-yaml-config": "^4.0.3",
    "styled-components": "^4.0.3",
    "styled-normalize": "^8.0.3",
    "stylelint": "^10.0.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,jsx}": [
